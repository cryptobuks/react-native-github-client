{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": [
      "error",
      {
        devDependencies: ["storybook/**"]
      }
    ],
    "import/extensions": ["always", { "js": "never" }],
    "no-use-before-define": ["error", { "variables": false }]
  }
}
